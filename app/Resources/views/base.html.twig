<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>{% block title %}Spike Team Notifier{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="{{ asset('css/libraries/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/libraries/font-awesome.min.css') }}" rel="stylesheet">
        <link tyep="text/css" href="{{ asset('css/stylesheet.css') }}" rel="stylesheet">
    </head>
    <body class ="{% block bodySection %}anon{% endblock %}">
        {% block navbar %}{% endblock %}
        <div id="body">
            <div id="inner">
                {% block body %}{% endblock %}
            </div>
        </div>
        {% block javascripts %}
            <script src="{{ asset('js/libraries/jquery.min.js') }}"></script>
            <script src="{{ asset('js/libraries/bootstrap.min.js') }}"></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
            <script src="{{ asset('js/app.js') }}"></script>
            <script src="{{ asset('js/button.js') }}"></script>
            <script src="{{ asset('js/groups.js') }}"></script>
            <script src="{{ asset('js/stats.js') }}"></script>
        {% endblock %}
    </body>
</html>
